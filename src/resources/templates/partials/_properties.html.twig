<TR>
    <TD BORDER="{{ theme.tableBorder }}" ALIGN="LEFT" BGCOLOR="{{ theme.properties.background }}">
        {% if definition.hasProperties %}
            {% for constant in definition.constants|default([]) %}
                {% include 'partials/_constant.html.twig' %}
            {% endfor %}
            {% for property in definition.properties %}
                {% include 'partials/_property.html.twig' %}
            {% endfor %}
        {% else %}
            &nbsp;
        {% endif %}
    </TD>
</TR>
